{
    "name": "Weather App",
    "description": "An App which displays weather information",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "./popup.html",
        "default_icon": "./icon128.png"
    },
    "icons": {
        "16": "./icon16.png",
        "48": "./icon48.png",
        "128": "./icon128.png"
    },
    "permissions": ["activeTab", "geolocation"]
}